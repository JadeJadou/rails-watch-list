<h1>All my lists</h1>

<% @lists.each do | list | %>
  <%= list.name %>
  <% list.photos.each do |photo| %>
    <%= cl_image_tag photo.key, width: 400, height: 300, crop: :fill %>
  <% end %>
  <br>
  <%= link_to "Have a look a this list", list_path(list) %>
  <br>
<% end %>

    <%= link_to "Create a list", new_list_path %>
<br>


